<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arturdevmob.keepmoney">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.arturdevmob.keepmoney.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <activity android:name="com.arturdevmob.keepmoney.ui.SingleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.arturdevmob.keepmoney.ui.accounts.detail.transaction.edit.TransactionEditActivity"
            android:label="@string/new_transaction" />
        <activity
            android:name="com.arturdevmob.keepmoney.ui.accounts.edit.AccountsEditActivity"
            android:label="@string/add_account" />
        <activity
            android:name="com.arturdevmob.keepmoney.ui.settings.category.edit.CategoryEditActivity"
            android:label="@string/add_category" />
        <activity android:name="com.arturdevmob.keepmoney.ui.accounts.detail.transaction.edit.choicecategory.ChoiceCategoryActivity"
            android:label="@string/choice_category"/>
    </application>

</manifest>